nie.define(function(){"use strict";var e=window.location.href;e.indexOf("uu.163.com")>0&&$(".center_nav li").eq(0).addClass("active").siblings("li").removeClass("active"),e.indexOf("games")>0&&$(".center_nav li").eq(1).addClass("active").siblings("li").removeClass("active"),e.indexOf("ie.uu.163.com/channel/order/website")>0&&$(".center_nav li").eq(2).addClass("active").siblings("li").removeClass("active"),e.indexOf("contact")>0&&$(".center_nav li").eq(6).addClass("active").siblings("li").removeClass("active"),e.indexOf("event")>0&&$(".center_nav li").removeClass("active"),e.indexOf("news")>0&&$(".center_nav li").removeClass("active"),e.indexOf("/down/android/")>0&&$(".center_nav li").removeClass("active"),e.indexOf("/down/ios/")>0&&$(".center_nav li").removeClass("active"),e.indexOf("/down/mac/")>0&&$(".center_nav li").removeClass("active"),e.indexOf("/router/")>0&&$(".center_nav li").removeClass("active")});