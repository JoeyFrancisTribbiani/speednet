{
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": false,
    "semi": false,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "printWidth": 100,
    "proseWrap": "always",
    "vueIndentScriptAndStyle": false,
    "quoteProps": "as-needed",
    "htmlWhitespaceSensitivity": "css"
}
